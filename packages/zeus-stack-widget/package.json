{
  "name": "@zeus-network/zeus-stack-widget",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeusNetworkHQ/zeus-stack-widget.git"
  },
  "bugs": {
    "url": "https://github.com/ZeusNetworkHQ/zeus-stack-widget/issues"
  },
  "type": "module",
  "private": false,
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./bitcoin-wallet-adapter": {
      "types": "./dist/bitcoin.d.ts",
      "import": "./dist/bitcoin.js",
      "require": "./dist/bitcoin.cjs"
    },
    "./assets/*": {
      "import": "./dist/assets/*",
      "require": "./dist/assets/*"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && vite build",
    "build:dev": "TSX_COMPILE_ON_ERROR=true ESLINT_NO_DEV_ERRORS=true tsc && NODE_ENV=development vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "@solana/wallet-adapter-react": {
      "optional": false
    },
    "@solana/wallet-adapter-react-ui": {
      "optional": false
    }
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.5.2",
    "@zeus-network/bitcoin-wallet-adapter": "workspace:*",
    "@zeus-network/client": "workspace:*",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.3.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-popover": "1.1.6",
    "@solana/web3.js": "^1.91.8",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1",
    "vite-plugin-node-polyfills": "^0.24.0",
    "zod": "^3.25.75"
  }
}
